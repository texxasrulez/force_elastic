{
  "name": "texxasrulez/force_elastic",
  "description": "Roundcube webmail plugin that forces the Elastic skin on phones and tablets at runtime (no user preference changes).",
  "type": "roundcube-plugin",
  "license": "proprietary",
  "version": "1.0",
  "homepage": "https://github.com/texxasrulez/force_elastic",
  "support": {
    "source": "https://github.com/texxasrulez/force_elastic",
    "issues": "https://github.com/texxasrulez/force_elastic/issues",
    "docs": "https://github.com/texxasrulez/force_elastic#readme"
  },
  "authors": [
    {
      "name": "texxasrulez",
      "email": "texxasrulez@yahoo.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "roundcube/plugin-installer": "^0.3.0"
  },
  "extra": {
    "roundcube": {
      "min-version": "1.4"
    }
  },
  "keywords": [
    "roundcube",
    "plugin",
    "skin",
    "elastic",
    "mobile",
    "responsive",
    "webmail",
    "roundcube-plugin",
    "mobile-detection",
    "ux",
    "ui"
  ],
  "autoload": {
    "classmap": [
      "."
    ]
  }
}
